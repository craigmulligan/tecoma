:8080

log
respond / "All good in the hood" 200 

handle_path /router {
  rewrite * /{path}
  reverse_proxy {
    to 192.168.1.1:80
    header_up Host {http.reverse_proxy.upstream.host}
    header_up X-Real-IP {remote}
    header_up X-Forwarded-Proto {scheme}
  }
}

handle_path /* {
  reverse_proxy {
    to 192.168.1.1
  }
}
